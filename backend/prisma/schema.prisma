datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}


model Note {
  id         Int      @id @default(autoincrement())
  title      String
  content    String
  tags       String[]
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
